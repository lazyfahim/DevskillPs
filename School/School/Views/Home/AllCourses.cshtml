@model System.Collections.Generic.IList<School.Models.Course>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>title</h2>
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    @foreach (var course in Model)
    {
        <a asp-controller="Home" asp-action="Details" asp-route-Id="@course.Id">
            <h2>
                @course.Title | @course.Fee | @course.SeatCount 
            </h2>
        </a>
        <a asp-controller="Home" class="btn btn-danger" asp-action="Delete" asp-route-Id="@course.Id">DELETE</a>
    }
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>