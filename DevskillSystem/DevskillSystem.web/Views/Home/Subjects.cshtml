@model System.Collections.Generic.IList<DevSkillSystem.FrameWork.Subject>

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>title</h2>
@foreach (var subject in Model)
{
    <a asp-action="Subject"> <h3>@subject.Name</h3> | <h4> Subject Open: @subject.RegistrationOpen</h4> </a>
    <a asp-action="DeleteSubject" asp-route-Id="@subject.Id" class="btn btn-danger">Delete</a>

}
